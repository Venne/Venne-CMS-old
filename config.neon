# generated by Nette

global:
	mode: detect
	developerIp: null
	
common:
	php:
		date.timezone: Europe/Prague
		
	services:
		robotLoader:
			run: true
			
		ITranslator: \Nella\Localization\Translator
		
	database:
		driver: pdo_mysql
		host: host
		dbname: dbname
		user: user
		password: pass
		charset: utf8
		driverOptions:
			1002: SET NAMES utf8
			
		proxyDir: %appDir%/proxy
		proxyNamespace: proxy
		
	smtp:
		host: smtp.gmail.com
		username: username
		password: pass
		secure: ssl
		
	venne:
		lang: cs
		template: venne
		defaultModule: pages
		repositories:
			core:
				mirrors:
					- http://repo.venne.cz/core/
					
				name: anonym
				password: ""
				
		panels:
			- stopwatch
			
		admin:
			name: name
			password: pass
			
		modules:
			callbackModule:
				run: true

			modulesModule:
				run: true
				
			systemModule:
				run: true
				
			sitemapModule:
				run: true
				routePrefix: ""
				
			usersModule:
				run: true
				
			navigationModule:
				run: true
				
			aliasModule:
				run: true
				
			commentsModule:
				run: true
				
			pagesModule:
				run: true
				routePrefix: ""
				sitemapPriority: 0.45
				
			websiteModule:
				run: true
				
			languageModule:
				run: true
				
		macros:
			- css
			- js
			- element
			- confirm
			
		helpers: null
		
	
production < common: 
development < common: 
console < common: 
